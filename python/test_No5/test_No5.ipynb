    "#importing modules\n",
    "import nltk\n",
    "from nltk.tokenize import word_tokenize \n",
    "from nltk.tokenize.punkt import PunktSentenceTokenizer\n",
    "\n",
    "import pymorphy2\n",
    "import string\n",
    "from collections import Counter\n",
    "import operator\n",
    "import matplotlib.pyplot as plt"
   ]
  },
  {
   "cell_type": "code",
